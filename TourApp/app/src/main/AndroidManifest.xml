        </activity>
        <activity
            android:name=".Hotels"
            android:label="Hotels">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.andriod.tourapp.MainActivity" />
    </activity>
        <activity
            android:name=".Labadi"
            android:label="Labadi Beach Hotel" >
